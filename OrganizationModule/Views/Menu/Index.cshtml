@using AppEngine.Models.DataBusiness
@switch ((ProfileEnum)ViewBag.LoggedUser.Profile)
{
    case ProfileEnum.Administrator:
        @Html.Action("AdminMenu", "Menu")
        break;
    case ProfileEnum.Creator:
        @Html.Action("CreatorMenu", "Menu")
        break;
    case ProfileEnum.Manager:
        @Html.Action("ManagerMenu", "Menu")
        break;
    case ProfileEnum.Protector:
        @Html.Action("KeeperMenu", "Menu")
        break;
    case ProfileEnum.User:
        @Html.Action("UserMenu", "Menu")
        break;
    case ProfileEnum.Superuser:
        @Html.Action("KeeperMenu", "Menu")
        break;
    default:
        break;
} 